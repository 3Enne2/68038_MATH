
syms x y z;                       % Define symbolic variables
F = [5*y*z, 5*x*z, 5*x*y];        % Define the vector field F
divF = divergence(F, [x, y, z]);  % Calculate the divergence of F
curF = curl(F, [x, y, z]);        % Calculate the curl of F
disp('Divergence of F:');         % Display the results
disp(divF);
disp('Curl of F:');
disp(curF);
